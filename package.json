{
  "name": "transforma",
  "version": "0.0.1",
  "description": "Intelligent, AI-assisted data mapping and transformation for complex, automated data workflows",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "tsx": "^4.20.3",
    "turbo": "^2.5.5",
    "typescript": "5.8.3"
  },
  "engines": {
    "node": ">=22"
  },
  "packageManager": "npm@8.3.1",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zjcompt/transforma.git"
  },
  "author": "Zach Compton",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zjcompt/transforma/issues"
  },
  "homepage": "https://github.com/zjcompt/transforma#readme"
}
